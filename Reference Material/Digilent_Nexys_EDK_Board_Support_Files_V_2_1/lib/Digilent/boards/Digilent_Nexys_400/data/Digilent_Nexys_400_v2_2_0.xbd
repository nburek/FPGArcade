BEGIN FPGA
  ATTRIBUTE INSTANCE = fpga_0
  ATTRIBUTE EXCLUSIVE = FPGA
  ATTRIBUTE FAMILY = spartan3
  ATTRIBUTE  DEVICE =  XC3S400
  ATTRIBUTE PACKAGE =  FT256
  ATTRIBUTE SPEED_GRADE =  -4
  ATTRIBUTE JTAG_POSITION = 1
          
