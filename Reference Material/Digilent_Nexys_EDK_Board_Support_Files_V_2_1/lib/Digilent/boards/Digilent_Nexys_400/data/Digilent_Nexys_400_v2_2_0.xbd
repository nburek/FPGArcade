   #PORT TXD_JC  = uart1_sout_JC, UCF_NET_STRING=("LOC=A7", "DRIVE=2", "SLEW=SLOW", "IOSTANDARD = LVCMOS33")
   
   ### UART to JD connector ###
   #PORT RXD_JD  = uart1_sin_JD, UCF_NET_STRING=("LOC=C10", "DRIVE=2", "SLEW=SLOW", "IOSTANDARD = LVCMOS33")
   #PORT TXD_JD  = uart1_sout_JD, UCF_NET_STRING=("LOC=D12", "DRIVE=2", "SLEW=SLOW", "IOSTANDARD = LVCMOS33")

     
