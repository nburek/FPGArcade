  ### RESET ###
  PORT RESET = CONN_FPGA.RESET, UCF_NET_STRING=("LOC=j13",  "IOSTANDARD = LVCMOS33")

   ### UART to JA connector ###
   PORT RXD_JA  = uart1_sin_JA, UCF_NET_STRING=("LOC=T13", "DRIVE=2", "SLEW=SLOW", "IOSTANDARD = LVCMOS33")
   PORT TXD_JA  = uart1_sout_JA, UCF_NET_STRING=("LOC=R12", DRIVE=2", "SLEW=SLOW", "IOSTANDARD = LVCMOS33")
   
   ### UART to JB connector ###
